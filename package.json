{
  "name": "@takinprofit/lazydb",
  "version": "1.0.2",
  "description": "A modern Node.js embedded database library with advanced querying capabilities.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "pkgroll --tsconfig tsconfig.build.json",
    "test:all": "tsx --test src/**/*.test.ts",
    "test": "npm-run-all2 clean test:all",
    "clean": "pnpm rimraf dist coverage advanced-test-db/*.mdb test-db/*.mdb",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "database",
    "lmdb",
    "typescript",
    "modern",
    "query"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.9.0",
    "npm-run-all2": "^7.0.1",
    "pkgroll": "^1.0.0",
    "rimraf": "^5.0.10",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "lmdb": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Takin-Profit/LazyDB.git"
  },
  "bugs": {
    "url": "https://github.com/Takin-Profit/LazyDB/issues"
  },
  "homepage": "https://github.com/Takin-Profit/LazyDB",
  "files": [
    "dist/",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your_username"
  }
}
